{
  "name": "toom_forum",
  "version": "1.0.0",
  "description": "Servidor Web para mi App de Toom Forum",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test jest --verbose --runInBand",
    "test:coverage": "NODE_ENV=test jest --coverage --verbose --runInBand",
    "start": "NODE_ENV=production node build/src/index.js",
    "start:test": "NODE_ENV=test node build/src/index.js",
    "tsc": "tsc",
    "dev": "NODE_ENV=development ts-node-dev --files src/index.ts",
    "lint": "eslint --ext .ts .",
    "build:ui": "rm -rf build && cd ../frontend && npm run build && cp -r build ../backend",
    "deploy": "git push heroku main",
    "deploy:full": "npm run build:ui && git add . && git commit -m deploying && npm run deploy",
    "logs:prod": "heroku logs --tail"
  },
  "keywords": [
    "Animal",
    "Crossing",
    "Express"
  ],
  "author": "Linuxgunter",
  "license": "ISC",
  "dependencies": {
    "@types/multer": "^1.4.7",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "pg": "^8.7.3",
    "sequelize": "^6.19.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "ts-node-dev": "^1.1.8"
  }
}
